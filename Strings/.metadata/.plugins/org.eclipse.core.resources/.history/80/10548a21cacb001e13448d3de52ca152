package Basics;
import java.util.Scanner;
public class ReverseString {

	public static void main(String[] args) {
		
		Scanner in = new Scanner(System.in);
		
		String str = in.next();
		String rev="";
		for(int i=str.length();i>0;i++) {
			rev = rev+str.charAt(i);
		}
		
		System.out.print(rev);
	}

}
